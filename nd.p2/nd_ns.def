// NS: TN -> NUT
//
////////////////////////////////

FEM_icmp6_ns(
	nd_ucast_ns,
	_HETHER_tn2nut,
	{
		HopLimit	= 255;
		_SRC(tnv6());
		_DST(nutv6());
	},
	{
		TargetAddress	= nutv6();
	}
)

